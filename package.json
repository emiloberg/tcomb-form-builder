{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "description": "Minimal React boilerplate",
  "scripts": {
    "start": "NODE_ENV=development node dev-server.js",
    "start:prod": "node dev-server.js",
    "lint": "eslint src",
    "test": "NODE_ENV=test karma start",
    "test:watch": "NODE_ENV=test karma start --no-single-run",
    "build": "rimraf dist && BUILDLANG=en NODE_ENV=production webpack --config webpack/webpack.config.js",
    "build:analyze": "rimraf dist && BUILDLANG=en NODE_ENV=production webpack --config webpack/webpack.config.js --json > webpackStats.json && npm run analyze-build",
    "build:all": "rimraf dist && node makeProduction.js",
    "analyze-build": "cat webpackStats.json | ./node_modules/webpack-bundle-size-analyzer/analyze-bundle-size",
    "babel-node": "babel-node --presets=es2015",
    "mon": "nodemon --exec npm run babel-node -- temp.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/emiloberg/react-boilerplate.git"
  },
  "keywords": [
    "react",
    "jsx",
    "boilerplate",
    "es6",
    "es2015"
  ],
  "author": "Emil Oberg",
  "license": "WTFPL",
  "bugs": {
    "url": "https://github.com/emiloberg/react-boilerplate/issues"
  },
  "homepage": "https://github.com/emiloberg/react-boilerplate",
  "devDependencies": {
    "autoprefixer": "^6.2.0",
    "babel-core": "^6.0.20",
    "babel-eslint": "^4.1.3",
    "babel-loader": "^6.0.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.0.15",
    "babel-preset-react": "^6.0.15",
    "babel-preset-stage-0": "^6.0.15",
    "chai": "^3.4.1",
    "css-loader": "^0.23.0",
    "deep-freeze": "0.0.1",
    "deepmerge": "^0.2.10",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^2.1.1",
    "eslint-loader": "^1.2.0",
    "eslint-plugin-react": "^3.11.3",
    "expect": "^1.13.4",
    "expect-jsx": "^2.2.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "fetch-mock": "^4.0.1",
    "html-webpack-plugin": "^2.6.5",
    "i18n-webpack-plugin": "^0.2.7",
    "ignore-styles": "^1.2.0",
    "isparta": "^4.0.0",
    "isparta-instrumenter-loader": "^1.0.0",
    "isparta-loader": "^2.0.0",
    "istanbul-instrumenter-loader": "^0.1.3",
    "karma": "^0.13.18",
    "karma-chrome-launcher": "^0.2.2",
    "karma-cli": "^0.1.2",
    "karma-coverage": "^0.5.3",
    "karma-mocha": "^0.2.1",
    "karma-phantomjs-launcher": "^0.2.3",
    "karma-sourcemap-loader": "^0.3.6",
    "karma-spec-reporter": "0.0.23",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.3.4",
    "node-sass": "^3.4.2",
    "phantomjs": "^1.9.19",
    "postcss-loader": "^0.8.0",
    "postcss-nested": "^1.0.0",
    "react-addons-test-utils": "^0.14.5",
    "react-hot-loader": "^1.3.0",
    "redbox-react": "^1.2.2",
    "redux-devtools": "^3.0.0",
    "redux-devtools-dock-monitor": "^1.0.1",
    "redux-devtools-log-monitor": "^1.0.1",
