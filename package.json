    "style-loader": "^0.13.0",
    "toolbox-loader": "0.0.3",
    "webpack": "^1.12.2",
    "webpack-bundle-size-analyzer": "^1.3.0",
    "webpack-dev-server": "^1.12.1"
  },
  "dependencies": {
    "classnames": "^2.2.4",
    "clone": "^1.0.2",
    "isomorphic-fetch": "^2.2.0",
    "moment": "^2.11.2",
    "normalize.css": "^3.0.3",
    "object-path": "^0.9.2",
    "react": "^0.14.7",
    "react-addons-css-transition-group": "^0.14.6",
    "react-dnd": "^2.1.4",
    "react-dnd-html5-backend": "^2.1.2",
    "react-dom": "^0.14.7",
    "react-functional": "^1.2.0",
